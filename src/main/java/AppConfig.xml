<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:component-scan base-package="com.sky.ConstructorInjectionWithCollection"></context:component-scan>
  <bean id="employee" class="com.sky.ConstructorInjectionWithCollection.Employee">
    <constructor-arg value="101"></constructor-arg>
    <constructor-arg value="Akash"></constructor-arg>
    <constructor-arg>
    <list>
    <value>Java 8 and Above</value>
    <value>Spring Boot</value>
    <value>MySQL</value>
    </list>
    </constructor-arg>
  </bean>

 

</beans>